@page
@model LaptopShowModel
@{
    ViewData["Title"] = "LaptopShow";
}

@if(Model.LaptopsTable.Count == 0)
{
    <div class="alert alert-info" role="alert">No laptops found!</div>
}
else
{
    <table class="table table-striped">
        <thead class="thead-dark">
            <tr>
                <th scope="col">ID</th>
                <th scope="col">State</th>
                <th scope="col">Model</th>
                <th scope="col">Producer</th>
            </tr>
        </thead>
        @foreach(var question in Model.LaptopsTable)
        {
            <tr>
                <td>@question.ID</td>
                <td>@question.State</td>
                <td>@question.Model</td>
                <td>@question.Producer</td>
            </tr>

        }
    </table>
}